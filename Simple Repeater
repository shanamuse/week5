#include <stdio.h>

int main() {
    char symbol;
    int count;

    printf("ASCII Symbol?\n");
    scanf(" %c", &symbol);  
    
    printf("Repeat Count?\n");
    scanf("%d", &count);

    for (int i = 0; i < count; i++) {
        printf("%c", symbol);
        if (i < count - 1) {
            printf(" ");  
        }
    }

    printf("\n");  

    return 0;
}
